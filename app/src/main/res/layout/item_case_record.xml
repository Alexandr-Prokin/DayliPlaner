<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cardViewCalendar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    app:cardElevation="5dp"
    app:cardCornerRadius="10dp"
    android:foreground="?selectableItemBackground">
<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp">

    <TextView
        android:id="@+id/textViewDataStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="14:00" />

    <TextView
        android:id="@+id/textViewDataFinish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/textViewDataStart"
        app:layout_constraintTop_toTopOf="@+id/textViewDataStart"
        tools:text="15:00"
        android:layout_marginHorizontal="12dp"/>

    <TextView
        android:id="@+id/textViewNameCase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Description"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toEndOf="@+id/textViewDataFinish"
        app:layout_constraintTop_toTopOf="@+id/textViewDataFinish" />
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>